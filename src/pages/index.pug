html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Unistory | Home page
    link(rel="stylesheet", href="../styles/main.scss")
  body
    header.header
      .container
        a.header__logo(href="/")
          img(src="../icons/logo.svg")
        nav.mneu.header__menu
          ul.menu__list
            li.menu__item
              a(href="javascript:;", scroll-to="services") Services
            li.menu__item
              a(href="javascript:;", scroll-to="competence") Competence
            li.menu__item
              a(href="portfolio.pug") Portfolio
            li.menu__item
              a(href="javascript:;", scroll-to="about") About
            li.menu__item
              a.btn.btn_animated_blue(
                href="javascript:;",
                scroll-to="contactUs"
              ) Contact us
            twirl-burger 
    section.welcome
      .container 
        h1.welcome__title 
          | Software &nbsp;
          br.welcome__mobile-br
          .typed
          br
          | & &nbsp;
          span product consultance
        ul.welcome__advantages
          li.welcome__advantages-item 
            a.advantage(href="https://www.upwork.com/ag/neca/", target="blank")
              .advantage__left 
                span#upworkRating 4.9
                span 4.9
              .advantage__right
                img(src="../icons/Upwork.png", alt="upwork")
          li.welcome__advantages-item 
            a.advantage
              .advantage__left 
                span#developedProjects 20+
                span 20+
              .advantage__right
                | Developed
                br
                | projects
          li.welcome__advantages-item 
            a.advantage
              .advantage__left
                span#repeatCustomers 87%
                span 87%
              .advantage__right
                | Repeat
                br
                | customers
        button.btn.btn_animated_blue.btn_size_l.welcome__contact-btn(
          scroll-to="contactUs"
        ) Discuss the project
