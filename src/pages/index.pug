    section.cases-section
      .container 
        h2#competence.title Success stories
        .cases-section__grid
          article.cases-section__item.case-card.case-card_animation_scale
            ul.case-card__tags 
              li
                a(href="javascript:;", data-tag) #DeFi
              li
                a(href="javascript:;", data-tag) #eCommerce
              li
                a(href="javascript:;", data-tag) #Design
            a.case-card__img(href="javascript:;")
              img(src="../img/Case_1.png", alt="3Dprint.HK")
            h3.case-card__title 3Dprint.HK
          article.cases-section__item.case-card.case-card_animation_scale
            ul.case-card__tags 
              li
                a(href="javascript:;", data-tag) #NFT
              li
                a(href="javascript:;", data-tag) #Design
            a.case-card__img(href="javascript:;")
              img(src="../img/Case_2.jpeg", alt="Millionpieces")
            h3.case-card__title Puzzle People
          article.cases-section__item.case-card.case-card_animation_scale
